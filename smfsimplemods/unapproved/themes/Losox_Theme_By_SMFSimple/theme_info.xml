<?xml version="1.0"?>
<theme-info xmlns="http://www.simplemachines.org/xml/theme-info" xmlns:smf="http://www.simplemachines.org/">
	<!-- For the id, always use something unique - put your name, a colon, and then the package name. Smfsimple.com -->
	<id>Lean:Losox_Theme_By_SMFSimple</id>
	<version>3.0</version>
	<!-- Theme name, used purely for aesthetics. -->
	<name>Losox Theme</name>
	<!-- Author: your email address or contact information. Smfsimple.com The name attribute is optional. -->
	<author name="lean">marafio@hotmail.com</author>
	<!-- Website... where to get updates and more information. -->
	<website>https://www.smfsimple.com</website>
	<!-- Template layers to use, defaults to "html,body". -->
	<layers>html,body</layers>
	<!-- Templates to load on startup. Smfsimple.com Default is "index". -->
	<templates>index</templates>
	<!-- Base this theme off another? Smfsimple.com Default is blank, or no.  It could be "default". -->
	<based-on>dafault curve theme</based-on>
</theme-info>
